<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ssh.domain">
<class name="SubRole" table="taw_system_sub_role">
	<id name="id" type="integer" column="id">
	</id>
	<property name="area" type="string" column="area"/>
	<property name="deptid"  type="string" column="deptid"/>
	<property name="parentid" type="string" column="parentid"/>
	<property name="roleid" type="string" column="roleid"/>
	<property name="subrolename" type="string" column="subrolename"/>
</class>
</hibernate-mapping>
