    <property name="FAX" type="string" column="FAX"/>
    <property name="MOBILE" type="string" column="MOBILE"/>
    <property name="OPERUSERID" type="string" column="OPERUSERID"/>
    <property name="PHONE" type="string" column="PHONE"/>
    <property name="REMARK" type="string" column="REMARK"/>
    <property name="SEX" type="string" column="SEX"/>
    <property name="USERDEGREE" type="string" column="USERDEGREE"/>
    <property name="USERID" type="string" column="USERID"/>
    <property name="USERNAME" type="string" column="USERNAME"/>
    <property name="OPERREMOTIP" type="string" column="OPERREMOTIP"/>
