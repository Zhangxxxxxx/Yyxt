    <property name="ISREST" type="string" column="ISREST"/>
    <property name="USERSTATUS" type="string" column="USERSTATUS"/>
    <property name="PORTALROLENAME" type="string" column="PORTALROLENAME"/>
    <property name="FLAG" type="integer" column="FLAG"/>
    <property name="FAIL_COUNT" type="integer" column="FAIL_COUNT"/>
