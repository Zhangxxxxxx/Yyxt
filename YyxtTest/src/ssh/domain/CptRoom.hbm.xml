<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ssh.domain">
<class name="CptRoom" table="taw_system_cptroom">
	<id name="id" type="integer" column="id">
	</id>
	<property name="roomname" type="string" column="roomname"/>
	<property name="manager"  type="string" column="manager"/>
	<property name="tempmanager" type="string" column="tempmanager"/>
	<property name="endtime" type="string" column="endtime"/>
	<property name="deptid" type="string" column="deptid"/>
	<property name="phone" type="string" column="phone"/>
	<property name="mobile" type="string" column="mobile"/>
	<property name="fax" type="string" column="fax"/>
	<property name="address" type="string" column="address"/>
	<property name="deleted" type="string" column="deleted"/>
	<property name="notes" type="string" column="notes"/>
	<property name="parentid" type="string" column="parentid"/>
	<property name="leaf" type="string" column="leaf"/>
</class>
</hibernate-mapping>
